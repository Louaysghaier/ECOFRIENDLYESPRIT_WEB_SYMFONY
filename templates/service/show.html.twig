{% extends 'base.html.twig' %}

{% block title %}Service{% endblock %}

{% block body %}
    <h1>Service</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Serviceid</th>
                <td>{{ service.serviceid }}</td>
            </tr>
            <tr>
                <th>Servicename</th>
                <td>{{ service.servicename }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ service.price }}</td>
            </tr>
            <tr>
                <th>Img</th>
                <td>{{ service.img }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_service_index') }}">back to list</a>

    <a href="{{ path('app_service_edit', {'serviceid': service.serviceid}) }}">edit</a>

    {{ include('service/_delete_form.html.twig') }}
{% endblock %}
